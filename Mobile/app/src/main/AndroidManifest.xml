<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nikita.mymoney">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".views.StartActiviti"
            android:label="@string/title_activity_start_activiti"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".views.Card"
            android:label="@string/title_activity_card"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".views.Cash"
            android:label="@string/title_activity_cash"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>